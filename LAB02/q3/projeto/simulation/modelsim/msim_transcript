# WARNING: No extended dataflow license exists
add wave -position end  sim:/lab02/x
wave create -driver freeze -pattern counter -startvalue 00 -endvalue 11 -type Range -direction Up -period 50ps -step 1 -repeat forever -range 1 0 -starttime 0ps -endtime 1000ps sim:/lab02/s
# Editable Wave items are not compatible with the wave window's standard undo-redo.  Standard undo-redo has been disabled.
wave create -driver freeze -pattern clock -initialvalue 0 -period 100ps -dutycycle 50 -starttime 0ps -endtime 1000ps sim:/lab02/a
add wave -position end -autoscale 1 -format Literal -height 17 -editable 2 Edit:/lab02/s
wave create -driver freeze -pattern clock -initialvalue 0 -period 200ps -dutycycle 50 -starttime 0ps -endtime 1000ps sim:/lab02/b
add wave -position end -autoscale 1 -format Literal -height 17 -editable 2 Edit:/lab02/s
wave create -driver freeze -pattern clock -initialvalue 0 -period 400ps -dutycycle 50 -starttime 0ps -endtime 1000ps sim:/lab02/c
wave create -driver freeze -pattern clock -initialvalue 0 -period 800ps -dutycycle 50 -starttime 0ps -endtime 1000ps sim:/lab02/d
add wave -position end -autoscale 1 -format Literal -height 17 -editable 2 Edit:/lab02/s
wave create -driver freeze -pattern counter -startvalue 00 -endvalue 11 -type Range -direction Up -period 25ps -step 1 -repeat forever -range 1 0 -starttime 0ps -endtime 1000ps sim:/lab02/s
run -all
add wave -position 0 -autoscale 1 -format Literal -height 17 -editable 2 Edit:/lab02/s
